<div class="row">
	<h1> HOME </h1>
</div>
<div class="row">
	<table class="table table-striped">
		<col width="20px">
		<tbody>
			<%@posts.each do |post|%>
				<tr> 
					<td><%= link_to post[0].name + " says:", user_path(post[0])%></td>
					<td><%=post[1]%></td>
				</tr>
			<%end%>
		</tbody>
	</table>
</div>
